@Library("belajar-jenkins-shared-library@master")_

import lib.*;

pipeline {
    agent any
    stages {
        stage("Hello World") {
            steps {
                script {
                    hello.hello()
                }
            }
        }
        stage("Hello Groovy") {
            steps {
                script {
                    Output.hello(this, "Groovy")
                }
            }
        }
    }
}